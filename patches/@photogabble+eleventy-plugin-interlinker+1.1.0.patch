@@ -0,0 +1,6 @@
+export function toHtmlString(value) {
+  if (typeof value === 'string') return value;
+  if (value?.documentElement?.outerHTML) return value.documentElement.outerHTML;
+  if (value?.outerHTML) return value.outerHTML;
+  return String(value ?? '');
+}
