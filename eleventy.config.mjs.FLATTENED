  console.log(
    `Interlinker: unresolved=${count} threshold=${maxUnresolved} action=${action}`
  )
  if (action === 'fail') {
    throw new Error(
      `Interlinker unresolved links (${count}) exceeded threshold (${maxUnresolved}).`
    )
  }
