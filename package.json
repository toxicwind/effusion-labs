{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "node utils/dev/eleventy-with-codeframes.mjs",
    "dev": "ELEVENTY_SERVE=true node utils/dev/eleventy-with-codeframes.mjs --serve",
    "format": "prettier --write \"**/*.{js,mjs,json,md,yml,css,njk}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,json,md,yml,css,njk}\"",
    "test": "c8 node test/integration/runner.spec.mjs",
    "test:watch": "node --watch test/integration/runner.spec.mjs",
    "check": "npm run format:check && npm run lint && npm run test",
    "lint": "npm run lint:links",
    "lint:links": "markdown-link-check -c oneoff/link-check.config.json README.md",
    "deps:playwright": "playwright install chromium || true",
    "mcp:dev": "node mcp-stack/gateway/server.mjs",
    "mcp:integration": "node mcp-stack/tests/integration.mjs",
    "mcp:test": "node mcp-stack/tests/smoke.mjs",
    "patch:ternary": "node utils/scripts/patch-njk-ternaries.mjs",
    "scan:ternary": "node utils/scripts/scan-njk-ternaries.mjs",
    "verify:patches": "node utils/scripts/validation/verify-patch-applied.mjs",
    "postinstall": "if [ \"${PATCH_PACKAGE_RUN:-1}\" != \"0\" ] && [ -x node_modules/.bin/patch-package ]; then patch-package; fi"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "6.0.4",
    "@11ty/eleventy-navigation": "1.0.4",
    "@11ty/eleventy-plugin-rss": "2.0.4",
    "@11ty/eleventy-plugin-vite": "7.0.0",
    "@babel/parser": "7.28.4",
    "@mozilla/readability": "0.6.0",
    "@photogabble/eleventy-plugin-interlinker": "1.1.0",
    "@playwright/test": "1.55.0",
    "@quasibit/eleventy-plugin-schema": "1.11.1",
    "@quasibit/eleventy-plugin-sitemap": "2.2.0",
    "@shikijs/markdown-it": "3.12.2",
    "@shikijs/transformers": "3.12.2",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.13",
    "acorn": "8.15.0",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "axios": "1.12.1",
    "c8": "10.1.3",
    "chalk": "5.6.2",
    "colorette": "2.0.16",
    "concurrently": "9.2.1",
    "daisyui": "5.1.12",
    "date-fns": "4.1.0",
    "fast-glob": "3.3.3",
    "fd-find": "1.2.0",
    "fs-extra": "11.3.1",
    "gray-matter": "4.0.3",
    "hast-util-select": "6.0.4",
    "jscodeshift": "^17.3.0",
    "jsdom": "^26.1.0",
    "libnpmsearch": "9.0.0",
    "lucide": "0.544.0",
    "luxon": "3.7.2",
    "magic-string": "0.30.19",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-attrs": "4.3.1",
    "markdown-it-footnote": "4.0.0",
    "markdown-link-check": "3.13.7",
    "meriyah": "6.1.4",
    "micromatch": "4.0.8",
    "natural": "8.1.0",
    "node-cache": "5.1.2",
    "nodemon": "3.1.10",
    "npm-registry-fetch": "19.0.0",
    "nunjucks": "3.2.4",
    "ora": "8.2.0",
    "p-limit": "7.1.1",
    "p-queue": "8.1.1",
    "p-retry": "7.0.0",
    "patch-package": "8.0.0",
    "picocolors": "1.1.1",
    "playwright-extra": "4.3.6",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^2.1.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "proxy-chain": "2.5.9",
    "recast": "0.23.11",
    "rehype-parse": "9.0.1",
    "rehype-remark": "10.0.1",
    "rehype-sanitize": "6.0.0",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "remark-smartypants": "3.0.2",
    "remark-stringify": "11.0.0",
    "shiki": "3.12.2",
    "slugify": "1.6.6",
    "tailwindcss": "4.1.13",
    "tree-cli": "0.6.7",
    "undici": "7.16.0",
    "unified": "11.0.5",
    "vite": "7.1.5",
    "yaml": "2.8.1",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
