{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "build": "npx @11ty/eleventy",
    "clean": "del-cli _site",
    "format:check": "dprint check -c dprint.json",
    "format": "dprint fmt -c dprint.json",
    "format:classes": "rustywind --write \"src/**/*.{njk,html,js,jsx,ts,tsx}\"",
    "format:all": "npm run format:classes && npm run format",
    "format:check:all": "npm run format:check",
    "lint": "npm-run-all --parallel lint:js lint:links",
    "lint:js": "eslint \"src/**/*.{js,mjs,ts,tsx}\" \"tools/**/*.mjs\" \"mcp-stack/**/*.mjs\" eleventy.config.mjs --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.{js,mjs,ts,tsx}\" \"tools/**/*.mjs\" \"mcp-stack/**/*.mjs\" eleventy.config.mjs --fix",
    "lint:links": "markdown-link-check -c oneoff/link-check.config.json README.md",
    "dead": "knip --reporter compact",
    "dead:prod": "knip --production --reporter compact",
    "dead:strict": "knip --strict --reporter compact",
    "dead:watch": "knip --watch --production",
    "dead:deps": "knip --production --dependencies --reporter compact",
    "dead:files": "knip --production --files --reporter compact",
    "dead:exports": "knip --production --exports --reporter compact",
    "dead:fix": "knip --production --fix --fix-type exports --format --reporter compact",
    "dead:fix:deps": "knip --production --fix --fix-type dependencies --format --reporter compact",
    "dead:json": "knip --production --reporter json > .knip-report.json",
    "dead:md": "knip --production --reporter markdown > .knip-report.md",
    "dead:ci": "knip --production --reporter compact",
    "dead:ci:soft": "knip --production --reporter compact --no-exit-code",
    "test": "c8 node test/integration/runner.spec.mjs",
    "test:watch": "node --watch test/integration/runner.spec.mjs",
    "test:playwright": "playwright test",
    "check": "npm run doctor && npm run format:check && npm run lint && npm run test",
    "doctor": "node tools/doctor.mjs",
    "lv-images:update": "node src/content/projects/lv-images/update-dataset.mjs",
    "lv-images:report": "node src/content/projects/lv-images/generate-lv-report.mjs",
    "mcp:dev": "node mcp-stack/gateway/server.mjs",
    "mcp:integration": "node mcp-stack/tests/integration.mjs",
    "mcp:test": "node mcp-stack/tests/smoke.mjs",
    "postinstall": "if [ \"${PATCH_PACKAGE_RUN:-1}\" != \"0\" ] && [ -x node_modules/.bin/patch-package ]; then patch-package; fi"
  },
  "lint-staged": {
    "package.json": [
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{js,mjs,ts,tsx}": [
      "eslint --fix --cache --cache-location .cache/eslint",
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{json,md,html,njk,yml,yaml}": [
      "dprint fmt -c dprint.json --allow-no-files"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "6.0.4",
    "@11ty/eleventy-navigation": "1.0.4",
    "@11ty/eleventy-plugin-rss": "2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/eleventy-plugin-vite": "7.0.0",
    "@babel/parser": "7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@mozilla/readability": "0.6.0",
    "@photogabble/eleventy-plugin-interlinker": "1.1.0",
    "@playwright/test": "1.55.0",
    "@quasibit/eleventy-plugin-schema": "1.11.1",
    "@quasibit/eleventy-plugin-sitemap": "2.2.0",
    "@shikijs/markdown-it": "3.12.2",
    "@shikijs/transformers": "3.12.2",
    "@tailwindcss/typography": "0.5.17",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "acorn": "8.15.0",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "1.12.2",
    "bloom-filters": "^3.0.4",
    "c8": "10.1.3",
    "chalk": "5.6.2",
    "colorette": "2.0.20",
    "concurrently": "9.2.1",
    "cssnano": "^7.1.1",
    "cssremedy": "^0.1.0-beta.2",
    "daisyui": "5.1.13",
    "date-fns": "4.1.0",
    "del-cli": "^7.0.0",
    "depcheck": "^1.4.7",
    "dprint": "^0.50.2",
    "eslint": "^9.36.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "fast-glob": "3.3.3",
    "fast-xml-parser": "^4.5.3",
    "fd-find": "1.2.0",
    "focus-visible": "^5.2.1",
    "fs-extra": "11.3.2",
    "globals": "^16.4.0",
    "gray-matter": "4.0.3",
    "hast-util-select": "6.0.4",
    "husky": "^9.1.7",
    "jscodeshift": "^17.3.0",
    "jsdom": "^27.0.0",
    "knip": "^5.64.0",
    "libnpmsearch": "9.0.0",
    "lint-staged": "^16.1.6",
    "lucide": "0.544.0",
    "luxon": "3.7.2",
    "magic-string": "0.30.19",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-attrs": "4.3.1",
    "markdown-it-footnote": "4.0.0",
    "markdown-link-check": "3.13.7",
    "meriyah": "6.1.4",
    "micromatch": "4.0.8",
    "natural": "8.1.0",
    "node-cache": "5.1.2",
    "nodemon": "3.1.10",
    "npm-registry-fetch": "19.0.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "3.2.4",
    "ora": "9.0.0",
    "p-limit": "^7.1.1",
    "p-queue": "8.1.1",
    "p-retry": "7.0.0",
    "patch-package": "8.0.0",
    "picocolors": "1.1.1",
    "playwright-extra": "4.3.6",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "proxy-chain": "2.5.9",
    "puppeteer": "^24.22.0",
    "recast": "0.23.11",
    "rehype-parse": "9.0.1",
    "rehype-remark": "10.0.1",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "remark-smartypants": "^3.0.2",
    "remark-stringify": "11.0.0",
    "rollup-plugin-critical": "^1.0.15",
    "rustywind": "^0.24.3",
    "sass": "^1.92.1",
    "shiki": "3.12.2",
    "slugify": "1.6.6",
    "tailwindcss": "4.1.13",
    "tree-cli": "0.6.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "undici": "^7.16.0",
    "unified": "11.0.5",
    "vite": "7.1.6",
    "yaml": "2.8.1",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">=22.19.0"
  },
  "dependencies": {
    "iconify-icon": "3.0.1"
  }
}
