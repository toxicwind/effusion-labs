{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "main": "index.js",
  "scripts": {
    "prepare": "[ -z \"$CI\" ] && bash scripts/llm-bootstrap.sh || true",
    "test": "c8 --reporter=text-summary --reporter=lcov node tools/runner.mjs",
    "test:all": "c8 --reporter=text-summary --reporter=lcov node tools/runner.mjs --all",
    "test:browser": "node --test \"test/browser/**/*.test.mjs\" --test-reporter=spec",
    "coverage:report": "c8 report --reporter=text-summary --reporter=lcov",
    "build": "npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve",
    "proxy:health": "node scripts/proxy-health.mjs",
    "docs:validate": "node tools/validate-docs.js",
    "docs:reindex": "node tools/build-vendor-index.js",
    "build:tools": "npm install --prefix tools/google-search && npm run build --prefix tools/google-search",
    "deps:playwright": "npx playwright install chromium || true",
    "deps:system": "npx playwright install-deps || true",
    "proxy:chain": "node tools/shared/chain-proxy.mjs",
    "prepare-docs": "npm exec fd --version || npm install --save-dev fd-find --progress || true",
    "docs:links": "find . -name '*.md' -not -path './node_modules/*' | xargs markdown-link-check -c link-check.config.json",
    "format": "prettier -w ."
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@photogabble/eleventy-plugin-interlinker": "^1.1.0",
    "jsdom": "^26.1.0",
    "lucide": "^0.534.0",
    "luxon": "^3.6.1",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-footnote": "^4.0.0",
    "undici": "^7.13.0"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "@playwright/test": "^1.54.2",
    "@quasibit/eleventy-plugin-schema": "1.11.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "c8": "10.1.3",
    "daisyui": "^5.0.50",
    "fd-find": "^1.2.0",
    "jsdom": "^26.1.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-link-check": "^3.13.7",
    "playwright-extra": "^4.3.6",
    "postcss": "^8.5.6",
    "prettier": "3.3.3",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "proxy-chain": "^2.5.0",
    "slugify": "1.6.6",
    "tailwindcss": "^4.1.11"
  }
}
