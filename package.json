    "fd-find": "1.2.0",
    "focus-visible": "^5.2.1",
    "fs-extra": "11.3.2",
    "globals": "^16.4.0",
    "gray-matter": "4.0.3",
    "hast-util-select": "6.0.4",
    "husky": "^9.1.7",
