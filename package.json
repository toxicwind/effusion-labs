{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "main": "index.js",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "format": "prettier -w .",
    "test": "c8 node tools/runner.mjs",
    "test:all": "npm test -- --all",
    "test:browser": "node --test \"test/browser/**/*.test.mjs\"",
    "coverage": "c8 report",
    "docs:links": "find -L . -name '*.md' -type f -not -path './node_modules/*' | xargs markdown-link-check -c link-check.config.json",
    "docs:validate": "node tools/validate-docs.js",
    "docs:reindex": "node tools/build-vendor-index.js",
    "tools:build-search": "npm install --prefix tools/google-search && npm run build --prefix tools/google-search",
    "deps:setup": "npm run deps:playwright && npm run deps:system",
    "deps:playwright": "playwright install chromium || true",
    "deps:system": "playwright install-deps || true",
    "proxy:health": "node scripts/proxy-health.mjs",
    "deps:provision": "bash scripts/provision-env.sh",
    "data:merge-sections": "node tools/merge-sections.mjs  --verbose",
    "data:synthesize": "node tools/synthesize.mjs --verbose",
    "data:merge-golden": "node tools/merge-golden.mjs --verbose",
    "data:all": "npm run data:merge-sections && npm run data:synthesize && npm run data:merge-golden",
    "audit:golden": "node tools/audit-identity.mjs --file src/content/archives/golden_record_20250819T081755Z.json --max-combo=3 --top=5 --verbose"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "@11ty/eleventy-img": "^6.0.4",
    "@photogabble/eleventy-plugin-interlinker": "^1.1.0",
    "@quasibit/eleventy-plugin-schema": "1.11.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "cli-progress": "3.12.0",
    "daisyui": "^5.0.50",
    "lucide": "^0.534.0",
    "luxon": "^3.6.1",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-footnote": "^4.0.0",
    "p-map": "7.0.3",
    "postcss": "^8.5.6",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "slugify": "1.6.6",
    "tailwindcss": "^4.1.11",
    "undici": "^7.13.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "ajv": "8.17.1",
    "ajv-formats": "^3.0.1",
    "c8": "10.1.3",
    "fd-find": "^1.2.0",
    "jsdom": "^26.1.0",
    "libnpmsearch": "9.0.0",
    "markdown-link-check": "^3.13.7",
    "natural": "^8.1.0",
    "npm-registry-fetch": "19.0.0",
    "playwright-extra": "^4.3.6",
    "prettier": "3.3.3",
    "proxy-chain": "^2.5.0"
  }
}
