{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npx @11ty/eleventy --serve",
    "serve": "npx @11ty/eleventy --serve",
    "serve:playwright": "node tools/serve-static.mjs",
    "prebuild": "node bin/quality.mjs apply",
    "build": "npm run build:site",
    "prebuild:site": "node bin/quality.mjs apply",
    "build:site": "NODE_ENV=production npx @11ty/eleventy",
    "prebuild:offline": "node bin/quality.mjs apply",
    "build:offline": "node tools/lv-images/pipeline.mjs build",
    "build:ci": "node bin/quality.mjs check --soft && NODE_ENV=production npx @11ty/eleventy",
    "clean": "del-cli _site",
    "quality": "node bin/quality.mjs",
    "quality:apply": "node bin/quality.mjs apply",
    "quality:check": "node bin/quality.mjs check",
    "quality:ci": "node bin/quality.mjs check --soft",
    "quality:report": "node bin/quality.mjs check --soft --knip-report",
    "lint": "npm run quality:check",
    "lint:ci": "npm run quality:ci",
    "format": "npm run quality:apply",
    "test": "node tools/check-chromium.mjs && c8 node test/integration/runner.spec.mjs",
    "test:watch": "node tools/check-chromium.mjs && node --watch test/integration/runner.spec.mjs",
    "test:playwright": "node tools/check-chromium.mjs && playwright test",
    "check": "npm run doctor && npm run quality:check && npm run test",
    "doctor": "node tools/doctor.mjs",
    "tools:apply-patches": "node tools/apply-patches.mjs --verbose",
    "tools:check-patches": "node tools/apply-patches.mjs --check",
    "crawl:pages": "node tools/lv-images/pipeline.mjs crawl --mode=pages",
    "full playwright crawl with LIVE PAGES ONLY(browser caching disabled)  + cache all scraped images and webpages and bundle into a tgz(online)": "node tools/lv-images/pipeline.mjs crawl-pages-images",
    "full playwright crawl with LIVE PAGES ONLY(browser caching disabled) + cache only webpages (no images) and bundle into a tgz(online)": "node tools/lv-images/pipeline.mjs crawl-pages",
    "hydrate verify and build from the tgz(offline)": "node tools/lv-images/pipeline.mjs hydrate-build",
    "full playwright crawl with caching(LIVE PAGES ONLY) disabled and bundle into a tgz(online) + hydrate verify and build from the tgz (offline) combined": "node tools/lv-images/pipeline.mjs cycle-pages",
    "links:check": "node tools/run-if-network.mjs markdown-link-check -c oneoff/link-check.config.json README.md",
    "links:ci": "node tools/run-if-network.mjs markdown-link-check -c oneoff/link-check.config.json README.md || true",
    "lint:dead": "npm run quality:report",
    "mcp:dev": "node mcp-stack/gateway/server.mjs",
    "mcp:integration": "node mcp-stack/tests/integration.mjs",
    "mcp:test": "node mcp-stack/tests/smoke.mjs",
    "postinstall": "node tools/apply-patches.mjs"
  },
  "lint-staged": {
    "package.json": [
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{js,mjs,ts,tsx}": [
      "eslint --fix --cache --cache-location .cache/eslint",
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{njk,html,js,jsx,ts,tsx}": [
      "rustywind --write"
    ],
    "*.{json,html,njk,yml,yaml}": [
      "dprint fmt -c dprint.json --allow-no-files"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
