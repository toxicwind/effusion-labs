{
  "name": "effusion_labs_final",
  "version": "1.0.0",
  "description": "This is a digital studio and knowledge base for Effusion Labs, built with Eleventy and the powerful `@photogabble/eleventy-plugin-interlinker`.",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run eleventy:serve",
    "serve": "npm run eleventy:serve",
    "watch": "npm run eleventy:watch",
    "bench": "DEBUG=Eleventy:Benchmark* eleventy",
    "eleventy:serve": "eleventy --serve",
    "eleventy:watch": "eleventy --watch",
    "eleventy:build": "eleventy",
    "prebuild": "npm run images:hydrate",
    "build": "npm run eleventy:build",
    "prebuild:ci": "npm run images:hydrate",
    "build:ci": "node tools/lv-images/pipeline.mjs build-gitactions",
    "prebuild:offline": "npm run images:hydrate",
    "build:offline": "node tools/lv-images/pipeline.mjs build-local-offline",
    "prebuild:full": "npm run images:sync",
    "build:full": "node tools/lv-images/pipeline.mjs build-local-fullinternet",
    "build-local-offline": "npm run build:offline",
    "build-local-fullinternet": "npm run build:full",
    "build-gitactions": "npm run build:ci",
    "clean": "del-cli _site",
    "apply:patches": "node tools/apply-patches.mjs --force",
    "verify:patches": "node tools/apply-patches.mjs --check",
    "images:hydrate": "node tools/lv-images/pipeline.mjs hydrate",
    "images:update": "node tools/lv-images/pipeline.mjs update",
    "images:bundle": "node tools/lv-images/pipeline.mjs bundle",
    "images:verify": "node tools/lv-images/pipeline.mjs verify",
    "images:normalize": "node tools/lv-images/pipeline.mjs normalize",
    "images:sync": "node tools/lv-images/pipeline.mjs sync",
    "images:stats": "node tools/lv-images/pipeline.mjs stats",
    "format": "dprint fmt -c dprint.json",
    "format:check": "dprint check -c dprint.json",
    "format:classes": "rustywind --write \"src/**/*.{njk,html,js,jsx,ts,tsx}\"",
    "format:all": "npm run format:classes && npm run format",
    "format:check:all": "npm run format:check",
    "lint": "eslint \"src/**/*.{js,mjs,ts,tsx}\" \"tools/**/*.mjs\" \"mcp-stack/**/*.mjs\" eleventy.config.mjs --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.{js,mjs,ts,tsx}\" \"tools/**/*.mjs\" \"mcp-stack/**/*.mjs\" eleventy.config.mjs --fix",
    "lint:links": "markdown-link-check -c oneoff/link-check.config.json README.md",
    "lint:ci": "npm run lint && npm run lint:links",
    "lint:js": "npm run lint",
    "dead": "npm run knip:prod",
    "dead:prod": "npm run knip:prod",
    "dead:strict": "npm run knip:strict",
    "dead:watch": "knip --watch --production",
    "dead:deps": "knip --production --dependencies --reporter compact",
    "dead:files": "knip --production --files --reporter compact",
    "dead:exports": "knip --production --exports --reporter compact",
    "dead:fix": "knip --production --fix --fix-type exports --format --reporter compact",
    "dead:fix:deps": "knip --production --fix --fix-type dependencies --format --reporter compact",
    "dead:json": "knip --production --reporter json > .knip-report.json",
    "dead:md": "knip --production --reporter markdown > .knip-report.md",
    "dead:ci": "npm run knip:ci",
    "dead:ci:soft": "npm run knip:ci",
    "knip": "knip --reporter compact",
    "knip:prod": "knip --production --reporter compact",
    "knip:strict": "knip --strict --reporter compact",
    "knip:ci": "knip --production --reporter compact --no-exit-code",
    "test": "c8 node test/integration/runner.spec.mjs",
    "test:watch": "node --watch test/integration/runner.spec.mjs",
    "test:ui": "playwright test",
    "check": "npm run verify:patches && npm run lint && npm run test",
    "doctor": "node tools/doctor.mjs",
    "ci:smoke": "npm run verify:patches && npm run build:ci",
    "lv-images:hydrate": "npm run images:hydrate",
    "lv-images:update": "npm run images:update",
    "lv-images:bundle": "npm run images:bundle",
    "lv-images:verify": "npm run images:verify",
    "lv-images:normalize": "npm run images:normalize",
    "lv-images:sync": "npm run images:sync",
    "lv-images:stats": "npm run images:stats",
    "postinstall": "node -e \"(async()=>{try{const {access}=await import('node:fs/promises');await access('tools/apply-patches.mjs');await import('./tools/apply-patches.mjs');}catch(error){if(error?.code==='ENOENT'){console.warn('Skipping patch apply (tools/apply-patches.mjs not present).');return;}console.error(error);process.exit(1);}})();\""
  },
  "lint-staged": {
    "package.json": [
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{js,mjs,ts,tsx}": [
      "eslint --fix --cache --cache-location .cache/eslint",
      "dprint fmt -c dprint.json --allow-no-files"
    ],
    "*.{json,md,html,njk,yml,yaml}": [
      "dprint fmt -c dprint.json --allow-no-files"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-fetch": "5.1.0",
    "@11ty/eleventy-img": "6.0.4",
    "@11ty/eleventy-navigation": "1.0.4",
    "@11ty/eleventy-plugin-rss": "2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/eleventy-plugin-vite": "7.0.0",
    "@babel/parser": "7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@mozilla/readability": "0.6.0",
    "@photogabble/eleventy-plugin-interlinker": "1.1.0",
    "@playwright/test": "1.55.1",
    "@quasibit/eleventy-plugin-schema": "1.11.1",
    "@quasibit/eleventy-plugin-sitemap": "2.2.0",
    "@shikijs/markdown-it": "3.12.2",
    "@shikijs/transformers": "3.13.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "acorn": "8.15.0",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "1.12.2",
    "beasties": "^0.3.5",
    "bloom-filters": "^3.0.4",
    "c8": "10.1.3",
    "chalk": "5.6.2",
    "colorette": "2.0.20",
    "concurrently": "9.2.1",
    "cssnano": "^7.1.1",
    "cssremedy": "^0.1.0-beta.2",
    "daisyui": "5.1.21",
    "date-fns": "4.1.0",
    "del-cli": "^7.0.0",
    "depcheck": "^1.4.7",
    "dprint": "^0.50.2",
    "eslint": "^9.36.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "fast-glob": "3.3.3",
    "fast-xml-parser": "^4.5.3",
    "fd-find": "1.2.0",
    "focus-visible": "^5.2.1",
    "fs-extra": "11.3.2",
    "globals": "^16.4.0",
    "gray-matter": "4.0.3",
    "hast-util-select": "6.0.4",
    "husky": "^9.1.7",
    "iconify-icon": "3.0.1",
    "jscodeshift": "^17.3.0",
    "jsdom": "^27.0.0",
    "knip": "^5.64.0",
    "libnpmsearch": "9.0.1",
    "lint-staged": "^16.2.0",
    "lucide": "0.544.0",
    "luxon": "3.7.2",
    "magic-string": "0.30.19",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-attrs": "4.3.1",
