{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["type", "brand", "line", "slug", "name"],
  "properties": {
    "type": { "const": "character" },
    "brand": { "const": "pop-mart" },
    "line": { "const": "the-monsters" },
    "slug": { "type": "string" },
    "name": { "type": "string" },
    "aliases": { "type": "array", "items": { "type": "string" } },
    "notes": { "type": "string" }
  },
  "additionalProperties": false
}
