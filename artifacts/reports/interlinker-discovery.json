{
  "eleventy": {
    "collections": ["featured","interactive","recentAll","archiveProducts","archiveCharacters","archiveSeries"],
    "globalData": ["buildTime","dailySeed","homepageCaps","build"],
    "shortcodes": ["callout","failbox","failitem"],
    "passthroughs": ["_redirects"]
  },
  "plugin": {
    "name": "@photogabble/eleventy-plugin-interlinker",
    "version": "1.1.0",
    "loaderSurfaces": ["cjs","esm"]
  },
  "scaffoldMap": [
    {"kind":"product","base":"archives/product","datasetKeys":["archiveProductsEn","archiveAllProducts","archiveProducts"],"keyFields":["slugCanonical","productSlug","slug","product_id","title"]},
    {"kind":"character","base":"archives/character","datasetKeys":["archiveCharactersEn","archiveAllCharacters","archiveCharacters"],"keyFields":["charSlug","name","title","slug"]},
    {"kind":"series","base":"archives/series","datasetKeys":["archiveSeriesEn","archiveAllSeries","archiveSeries"],"keyFields":["seriesSlug","title","slug"]},
    {"kind":"spark","base":"sparks","datasetKeys":["collections.sparks"],"keyFields":["fileSlug","data.title"]},
    {"kind":"concept","base":"concepts","datasetKeys":["collections.concepts"],"keyFields":["fileSlug","data.title"]},
    {"kind":"project","base":"projects","datasetKeys":["collections.projects"],"keyFields":["fileSlug","data.title"]},
    {"kind":"meta","base":"meta","datasetKeys":["collections.meta"],"keyFields":["fileSlug","data.title"]},
    {"kind":"work","base":"work","datasetKeys":["collections.work","collections.sparks","collections.concepts","collections.projects","collections.meta"],"keyFields":["fileSlug","data.title"]}
  ],
  "i18n": {"defaultLocale": "en","localePrefixes": false}
}
