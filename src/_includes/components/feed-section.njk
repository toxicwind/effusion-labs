{# Feed section: uses the local feedEntry macro #}
{% from "components/section.njk" import section %}
{% macro feedSection(title, url, items) -%}
  {% call section(title, url) %}
    <ul class="space-y-4">
      {% for item in items %}
        {{ feedEntry(item) }}
      {% endfor %}
    </ul>
  {% endcall %}
{%- endmacro %}